{
	"name": "VueTest",
	"private": true,
	"scripts": {
		"test": "grunt test && npm run test:unit",
		"test:unit": "jest",
		"coverage": "npm run test:unit",
		"codex:build-demos": "cd lib/codex && npm install && npm run build-demos -w codex-docs && cd ../.. && npm run codex:copy-demos",
		"codex:copy-demos": "npm run codex:copy-demos-js && npm run codex:copy-demos-css",
		"codex:copy-demos-js": "cp lib/codex/packages/codex-docs/dist/codex-demos.cjs.js resources/codex-demos/codex-demos.cjs.js",
		"codex:copy-demos-css": "cp lib/codex/packages/codex-docs/dist/style.css resources/codex-demos/style.css"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.11",
		"@vue/compat": "3.2.37",
		"@vue/compiler-sfc": "3.2.37",
		"@vue/test-utils": "^2.0.0",
		"@vue/vue3-jest": "27.0.0-alpha.4",
		"@wikimedia/codex": "0.4.3",
		"eslint-config-wikimedia": "^0.21.0",
		"eslint-plugin-jest": "^25.7.0",
		"grunt": "1.5.3",
		"grunt-banana-checker": "0.10.0",
		"grunt-eslint": "24.0.0",
		"grunt-stylelint": "0.18.0",
		"jest": "^27.4.7",
		"stylelint-config-wikimedia": "0.13.1",
		"vue": "3.2.37"
	}
}
