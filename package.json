{
	"name": "VueTest",
	"private": true,
	"scripts": {
		"test": "grunt test && npm run test:unit",
		"test:unit": "jest",
		"coverage": "npm run test:unit",
		"codex:build-demos": "[ -d lib/codex ] && npm run codex:build-demos:main || echo 'Exiting. Please see README instructions to run this command.'",
		"codex:build-demos:main": "cd lib/codex && npm install && npm run doc && cd ../.. && npm run codex:copy-demos",
		"codex:copy-demos": "npm run codex:copy-demos-js && npm run codex:copy-demos-css",
		"codex:copy-demos-js": "cp lib/codex/packages/codex-docs/dist/codex-demos.umd.js resources/codex-demos/codex-demos.umd.js",
		"codex:copy-demos-css": "copyfiles -f lib/codex/packages/codex-docs/dist/codex-demos*.css resources/codex-demos"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.11",
		"@vue/compat": "3.2.37",
		"@vue/compiler-sfc": "3.2.37",
		"@vue/test-utils": "^2.0.0",
		"@vue/vue3-jest": "27.0.0-alpha.4",
		"@wikimedia/codex": "0.6.2",
		"copyfiles": "^2.4.1",
		"eslint-config-wikimedia": "0.24.0",
		"eslint-plugin-jest": "^25.7.0",
		"grunt": "1.5.3",
		"grunt-banana-checker": "0.10.0",
		"grunt-eslint": "24.0.0",
		"grunt-stylelint": "0.18.0",
		"jest": "^27.4.7",
		"stylelint-config-wikimedia": "0.13.1",
		"vue": "3.2.37"
	}
}
