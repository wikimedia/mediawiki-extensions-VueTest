{
	"name": "VueTest",
	"author": [
		"Eric Gardner"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:VueTest",
	"descriptionmsg": "vuetest-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\VueTest\\": "includes/"
	},
	"config": {},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\VueTest\\Hooks::onBeforePageDisplay"
	},
	"MessagesDirs": {
		"VueTest": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"VueTestAlias": "VueTest.alias.php"
	},
	"SpecialPages": {
		"VueTest": "MediaWiki\\Extension\\VueTest\\SpecialVueTest"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "VueTest/resources"
	},
	"ResourceModules": {
		"ext.vueTest": {
			"dependencies": [
				"ext.vueTest.components",
				"vue",
				"mediawiki.api"
			],
			"messages": [],
			"styles": [],
			"scripts": [
				"init.js"
			],
			"es6": true,
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.vueTest.components": {
			"dependencies": [
				"@wikimedia/codex"
			],
			"messages": [
				"vuetest-app-welcome",
				"vuetest-app-intro",
				"vuetest-app-see-also"
			],
			"styles": [],
			"packageFiles": [
				"components/index.js",
				"components/App.vue",
				{
					"name": "components/icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconAdd"
					]
				}
			],
			"es6": true,
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.vueTest.codexdemo": {
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"localBasePath": "resources/codex-demos",
			"remoteExtPath": "VueTest/resources/codex-demos",
			"packageFiles": [
				"init.js",
				"Sandbox.vue",
				"codex-demos.umd.js",
				{
					"name": "icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconArrowNext",
						"cdxIconBold",
						"cdxIconRobot",
						"cdxIconTrash",
						"cdxIconPause",
						"cdxIconPlay"
					]
				}
			],
			"dirSpecificSkinStyles": {
				"ltr": {
					"default": "codex-demos.style.css",
					"vector": "codex-demos.style-legacy.css",
					"vector-2022": "codex-demos.style-legacy.css"
				},
				"rtl": {
					"default": "codex-demos.style-rtl.css",
					"vector": "codex-demos.style-legacy-rtl.css",
					"vector-2022": "codex-demos.style-legacy-rtl.css"
				}
			},
			"noflip": true,
			"dependencies": [
				"vue"
			],
			"es6": true,
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"manifest_version": 2
}
