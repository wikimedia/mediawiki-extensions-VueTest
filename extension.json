{
	"name": "VueTest",
	"author": [
		"Eric Gardner"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:VueTest",
	"descriptionmsg": "vuetest-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\VueTest\\": "includes/"
	},
	"config": {},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\VueTest\\Hooks::onBeforePageDisplay"
	},
	"MessagesDirs": {
		"VueTest": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"VueTestAlias": "VueTest.alias.php"
	},
	"SpecialPages": {
		"VueTest": "MediaWiki\\Extension\\VueTest\\SpecialVueTest"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "VueTest/resources"
	},
	"ResourceModules": {
		"ext.vueTest": {
			"dependencies": [
				"ext.vueTest.components",
				"vue",
				"mediawiki.api"
			],
			"messages": [],
			"styles": [],
			"scripts": [
				"init.js"
			],
			"es6": true
		},
		"ext.vueTest.components": {
			"dependencies": [
				"@wikimedia/codex"
			],
			"messages": [
				"vuetest-app-welcome",
				"vuetest-app-intro"
			],
			"styles": [],
			"packageFiles": [
				"components/index.js",
				"components/App.vue",
				{
					"name": "components/icons.json",
					"callback": "ResourceLoaderCodexModule::getIcons",
					"callbackParam": [
						"cdxIconArrowNext",
						"cdxIconBold",
						"cdxIconTrash",
						"cdxIconPause",
						"cdxIconPlay"
					]
				}
			]
		}
	},
	"manifest_version": 2
}
